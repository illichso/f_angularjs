+  angular.module('auction', ['ngRoute'])
+    .config(['$routeProvider', function ($routeProvider) {
+      var title = function (page) {
+        return page + ' | Auction';
+      };
+
+      $routeProvider
+        .when('/', {
+          templateUrl: 'views/home.html',
+          controller: 'HomeController',
+          controllerAs: 'ctrl',
+          title: title('Home')
+        })
+        .when('/search', {
+          templateUrl: 'views/search.html',
